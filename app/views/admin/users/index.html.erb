
<table class="users">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Last Login At</th>
  </tr>
  <% for user in @users %>
    <tr>
      <td class="user_icon"><%= link_to user.name, admin_user_path(user) %></td>
      <td><%=h user.email %></td>
      <td><%=h user.last_login_at %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New User", new_admin_user_path %></p>
